{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environment":
    {
      "type": "string",
      "allowedValues": [
        "dev", "prod"
      ]
    },
    "dev": {
      "name": "development",
        "type": "object",
          "value":{
            "name":"VNet1",
            "addressPrefixes": [
                {
                    "name": "firstPrefix",
                    "addressPrefix": "10.0.0.0/22"
                }
            ],
            "subnets":[
                {
                    "name": "firstSubnet",
                    "addressPrefix": "10.0.0.0/24"
                },
                {
                    "name":"secondSubnet",
                    "addressPrefix":"10.0.1.0/24"
                }
            ]
        }
    },
    "prod":
    {
      "name": "production",
      "type": "object",
      "value": {
          "storageAccountType": "Standard_GRS"
                }
    }
  },
"variables": {
    "templatelink": "[concat('https://raw.githubusercontent.com/mnsameer/scaling/master/',parameters('environment'),'deploy')]"
},
"resources": [
    {
        "apiVersion": "2015-01-01",
        "name": "nestedTemplate",
        "type": "Microsoft.Resources/deployments",
        "properties": {
            "mode": "incremental",
            "templateLink": {
                "uri": "[variables('templatelink')]",
                "contentVersion": "1.0.0.0"
            },
        
          "parameters": {
          "environment": {
            "value": "[parameters('environment')]"
          }
        }
        }
    }
]
}
